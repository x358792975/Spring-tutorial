<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="person"
		class="com.selflearning.spring.test.sample1.Person">
		<constructor-arg value="777" name="id"></constructor-arg>
		<constructor-arg value="Sean" name="name"></constructor-arg>
		<property name="taxId" value="123"></property>
		<property name="address" ref="address"></property>
	</bean>
	<bean id="address"
		class="com.selflearning.spring.test.sample1.Address">
		<constructor-arg name="street" value="111 Jewel Ave"></constructor-arg>
		<constructor-arg name="postcode" value="11367"></constructor-arg>
	</bean>
	<bean id="car" class="com.selflearning.spring.test.sample1.Car">
		<constructor-arg value="Audi"></constructor-arg>
		<constructor-arg value="Henan"></constructor-arg>
		<constructor-arg value="300000"></constructor-arg>
		<constructor-arg value="120"></constructor-arg>


	</bean>
</beans>

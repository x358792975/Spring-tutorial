<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<!-- 
		class: points to FactoryBean's class name
		property: properties of FactoryBean
		
		However, it returns the instance by FactoryBean's getObject() method
	 -->
	<bean id="car" class="com.selflearning.spring.test.sample1.factorybean.CarFactoryBean">
		<property name="brand" value="BMW"></property>
	</bean>

</beans>
